//Zadanie 1
#include <iostream>
#include <ctime>
using namespace std;

int main(){
    srand(time(NULL));
    const int rozmiar = 100;
    int tab[rozmiar];
    cout<<"to są liczby w tablicy: "<<endl;
    for (int i=0; i<100; i++){
        tab[i]= rand() % 91 + 10;
        cout<<tab[i]<<" ";
    }
    cout<<endl;
    cout<<"\nA to są liczby parzyste w przedziale od 35 do 65: "<<endl;
    for (int i=0; i<100; i++){
        if (i>=35 && i<=65){
            if(i % 2 == 0){
                cout<<i<<" ";
            }
        }    
    }
    return 0;
}
//Zadanie 2
#include <iostream>
